{
  "name": "postcss-font-awesome",
  "version": "0.2.1",
  "description": "PostCSS plugin to pull in a Font Awesome's icon unicode",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "font awesome"
  ],
  "author": "Dan Gamble <dangamble89@gmail.com>",
  "license": "MIT",
  "repository": "dan-gamble/postcss-font-awesome",
  "bugs": {
    "url": "https://github.com/dan-gamble/postcss-font-awesome/issues"
  },
  "homepage": "https://github.com/dan-gamble/postcss-font-awesome",
  "dependencies": {
    "postcss": "^5.0.14",
    "request": "^2.72.0"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "eslint": "^2.10.2",
    "eslint-config-postcss": "^2.0.2"
  },
  "scripts": {
    "test": "ava && eslint *.js",
    "install": "node ./generateIconConfig.js"
  },
  "eslintConfig": {
    "extends": "eslint-config-postcss/es5",
    "rules": {
      "max-len": [
        1,
        80,
        4
      ]
    }
  }
}
